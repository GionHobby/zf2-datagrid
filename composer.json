{
    "name": "synergy/synergydatagrid",
    "description": "Module to display data in grid using the jqGrid javascript plugin",
    "type": "synergy-module",
    "license": "BSD-3-Clause",
    "keywords": [
        "data grid",
        "jqgrid",
        "zf2",
        "module"
    ],
    "homepage": "http://developer.peleodiase.com/synergy-data-grid",
    "authors": [
        {
            "name": "Pele Odiase",
            "email": "peleodiase@yahoo.co.uk"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:odiaseo/synergy-common.git"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zendframework": "2.*",
        "doctrine/doctrine-orm-module": ">=0.5",
        "doctrine/doctrine-module": ">=0.7",
        "doctrine/orm": "2.*",
        "synergy/common": "1.0.x-dev",
        "monolog/monolog": "^1.18"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "suggest": {
        "gedmo/doctrine-extensions": ">=v2.2.0"
    },
    "autoload": {
        "psr-0": {
            "SynergyDataGrid": "src/",
            "SynergyDataGridTest": "tests/module"
        },
        "classmap": [
            "./Module.php",
            "tests/module/SynergyDataGridTest/Module.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
